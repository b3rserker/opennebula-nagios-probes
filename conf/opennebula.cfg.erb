#######################################################################################################################
## Default path for Nagios3 command definitions is:
##      /etc/nagios-plugins/config
## installed in:
##      <%= nagios_command_path %>
##
## Default path for Nagios3 plugins is:
##      /usr/lib/nagios/plugins
## installed in:
##      <%= nagios_plugin_path %>
##
## Default path to Ruby is:
##      ""    (i.e. empty, Ruby is provided by "/usr/bin/env ruby")
## installed with:
##      <%= ruby_command_path %>
#######################################################################################################################

# 'check_opennebula_oned_basic' command definition
define command{
    command_name    check_opennebula_oned_basic
    command_line    <%= ruby_command_path %> <%= nagios_plugin_path %>/check_opennebula/check_opennebula.rb --service oned --protocol http --hostname '$HOSTADDRESS$' --port '$ARG1$' --path '$ARG2$' --username '$ARG3$' --password '$ARG4$'
}

